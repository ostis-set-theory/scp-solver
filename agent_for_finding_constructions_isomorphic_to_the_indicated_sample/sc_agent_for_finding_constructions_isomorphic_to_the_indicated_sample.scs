sc_agent_for_finding_constructions_isomorphic_to_the_indicated_sample
=> nrel_main_idtf:
	[sc-агент поиска конструкций изоморфных указанному образцу] 
	(* <- lang_ru;; *);
	[sc-agent for finding constructions isomorphic to the indicated sample] 
	(* <- lang_en;; *);

<- abstract_sc_agent;

=> nrel_primary_initiation_condition: 
	(sc_event_add_output_arc => question_initiated);

=> nrel_initiation_condition_and_result: 
	(..sc_agent_for_finding_constructions_isomorphic_to_the_indicated_sample_condition => ..sc_agent_for_finding_constructions_isomorphic_to_the_indicated_sample_result);

<= nrel_sc_agent_key_sc_elements: 
	{
	question_initiated;
	question;
	question_for_finding_constructions_isomorphic_to_the_indicated_sample
	};

=> nrel_inclusion: 
	.platform_independent_realization_of_sc_agent_for_finding_constructions_isomorphic_to_the_indicated_sample
	(*
	<- platform_independent_abstract_sc_agent;;
	<= nrel_sc_agent_program: 
		{
		agent_for_finding_constructions_isomorphic_to_the_indicated_sample;
		proc_for_finding_constructions_isomorphic_to_the_indicated_sample	
		};;
	-> sc_agent_for_finding_constructions_isomorphic_to_the_indicated_sample_scp   
		(* <- active_sc_agent;; *);;
	*);;

..sc_agent_for_finding_constructions_isomorphic_to_the_indicated_sample_condition
= [*
	question_for_finding_constructions_isomorphic_to_the_indicated_sample _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_for_finding_constructions_isomorphic_to_the_indicated_sample_result
= [*
	question_for_finding_constructions_isomorphic_to_the_indicated_sample _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;